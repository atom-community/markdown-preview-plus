{
  "name": "markdown-preview-plus",
  "version": "2.4.16",
  "main": "./lib/main",
  "description": "Markdown Preview + Community Features",
  "keywords": ["markdown", "maths", "latex", "commonmark", "pandoc"],
  "repository": "https://github.com/atom-community/markdown-preview-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=1.23.0 <2.0.0"
  },
  "atomTestRunner": "./spec/runner.js",
  "scripts": {
    "build": "tsc -p .",
    "prettier": "prettier --write 'src/**/*.ts' 'spec/**/*.ts'",
    "prettier-check": "prettier -l 'src/**/*.ts' 'spec/**/*.ts'",
    "typecheck": "tsc --noEmit -p . && tsc --noEmit -p spec",
    "lint": "tslint --project . && tslint --project spec",
    "test": "npm run typecheck && npm run lint && npm run prettier-check"
  },
  "dependencies": {
    "MathJax":
      "https://github.com/atom-community/MathJax/tarball/2.4.0-electron.0.2.0",
    "atom-highlight": "^0.4.1",
    "fs-plus": "^3.0.1",
    "lodash": "^4.17.4",
    "markdown-it": "^8.4.0",
    "markdown-it-lazy-headers": "^0.1.2",
    "markdown-it-math":
      "https://github.com/atom-community/markdown-it-math/tarball/fix-table-pipe-parse-no-ascii",
    "markdown-it-task-lists": "^2.1.0",
    "season": "^6.0"
  },
  "devDependencies": {
    "@types/atom": "~1.23.0",
    "@types/chai": "^4.0.10",
    "@types/fs-extra": "^5.0.0",
    "@types/fs-plus": "^3.0.0",
    "@types/lodash": "^4.14.88",
    "@types/markdown-it": "0.0.4",
    "@types/mathjax": "0.0.32",
    "@types/mocha": "^2.2.45",
    "@types/node": "^7.0.0",
    "@types/sinon-chai": "^2.7.29",
    "@types/temp": "^0.8.31",
    "atom-haskell-tslint-rules": "^0.1.1",
    "atom-mocha-test-runner": "^1.2.0",
    "chai": "^4.1.2",
    "fs-extra": "^5.0.0",
    "jasmine-tagged": "^1.1.4",
    "prettier": "^1.9.2",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "temp": "^0.8.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "deserializers": {
    "MarkdownPreviewView": "createMarkdownPreviewView"
  }
}
